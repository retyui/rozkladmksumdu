    "@react-navigation/native": "^6.0.10",
    "axios": "^0.27.2",
    "mobx": "^6.5.0",
    "mobx-flipper": "^1.2.3",
    "mobx-react": "^7.4.0",
    "mobx-react-lite": "^3.4.0",
    "mobx-state-tree": "^5.1.4",
    "react": "17.0.2",
