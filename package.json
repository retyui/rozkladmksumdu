{
  "name": "rozkladmksumdu",
  "version": "2022.06.12.1-debug",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "start-reset-cache": "watchman watch-del-all && react-native start --reset-cache",
    "install-deps": "yarn install && cd ios && arch -x86_64 pod install && cd ..",
    "clean-cache": "watchman watch-del-all && cd ios && rm -f Podfile.lock && cd .. && rm -rf node_modules",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint-fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "android-port-revert": "adb reverse tcp:8081 tcp:8081",
    "debug-build-android": "cd android && ./gradlew assembleDebug",
    "predebug-build-android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "postdebug-build-android": "open ./android/app/build/outputs/apk/debug"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4",
    "@react-native-firebase/app": "^14.11.0",
    "@react-native-firebase/app-distribution": "^14.11.0",
